{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": "contract/.*\\.test\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": [
        "<rootDir>/../src/test-setup.ts"
    ]
}